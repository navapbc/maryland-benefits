{
  extends: 'nava',
  env: {
    browser: true,
    jest: true
  },
  rules: {
    // we rely on the side effect of constructors throwing on invalid props
    no-new: 'off',
    // we use es6 and eslint-config-nava is not es6 only yet
    prefer-const: 'error',
    no-var: 'error'
  },
  globals: {
  }
}
